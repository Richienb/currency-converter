{
    "name": "currency-converter",
    "version": "1.0.0",
    "description": "Simple currency converter build with React & Next.js",
    "private": true,
    "scripts": {
        "build": "next build",
        "dev": "next dev",
        "start": "next start",
        "test": "xo && stylelint './**/*.js'"
    },
    "engines": {
        "node": "12.x"
    },
    "xo": {
        "envs": [
            "node",
            "browser"
        ],
        "extends": "xo-react",
        "nodeVersion": ">=10",
        "parser": "babel-eslint",
        "rules": {
            "import/no-unassigned-import": "off"
        },
        "settings": {
            "react": {
                "version": "16.12"
            }
        }
    },
    "dependencies": {
        "cashify": "^2.2.0",
        "core-js": "^3.6.4",
        "framer-motion": "^1.7.0",
        "next": "^9.2.0",
        "prop-types": "^15.7.2",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-use-form-state": "^0.12.1",
        "styled-components": "^5.0.0",
        "swr": "^0.1.16"
    },
    "devDependencies": {
        "@babel/core": "^7.8.3",
        "babel-eslint": "^10.0.3",
        "babel-plugin-styled-components": "^1.10.6",
        "eslint-config-xo-react": "^0.22.0",
        "eslint-plugin-react": "^7.18.0",
        "eslint-plugin-react-hooks": "^2.3.0",
        "next-fonts": "^0.19.0",
        "next-offline": "^4.0.6",
        "stylelint": "^13.0.0",
        "stylelint-config-recommended": "^3.0.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-processor-styled-components": "^1.9.0",
        "xo": "^0.25.3"
    }
}
